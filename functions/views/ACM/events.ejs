		<%- include('footer.ejs') -%>
		<%- include('scripts.ejs') -%>
		<script>
			$(document).ready(function() {
				$("#myInput").on("keyup", function() {
					var value = $(this)
						.val()
						.toLowerCase();
					$("#myDIV *").filter(function() {
						$(this).toggle(
							$(this)
								.text()
								.toLowerCase()
								.indexOf(value) > -1
						);
					});
				});
			});
		</script>
	</body>
</html>
